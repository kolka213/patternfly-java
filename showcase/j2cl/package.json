{
  "name": "patternfly-showcase",
  "description": "Showcase for PatternFly Java",
  "private": true,
  "parcelIgnore": [
    "showcase.j2cl.js"
  ],
  "source": "src/web/index.html",
  "scripts": {
    "watch": "parcel serve --https --dist-dir target/showcase --open",
    "prod": "parcel build --dist-dir target/showcase --public-url \"./\"",

    "watch": "parcel serve --dist-dir target/todo --open",
    "dev": "parcel build --dist-dir target/todo --no-optimize",
    "prod": "parcel build --dist-dir target/todo --public-url /elemento/samples/todo/j2cl/"
  },
  "devDependencies": {
    "@parcel/core": "^2.12.0",
    "@parcel/reporter-cli": "^2.12.0",
    "parcel": "^2.12.0",
    "parcel-resolver-ignore": "^2.2.0"
  },
  "dependencies": {
    "@github/relative-time-element": "^4.3.1",
    "@patternfly/patternfly": "^5.2.1"
  }
}
